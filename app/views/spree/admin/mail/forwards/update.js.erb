<%if @response[:success] == true%>
  $("#myModal").modal('hide')
  $('#mail_forward_table #mail_forward_id_<%=@mail_forward.id%>').replaceWith("<%=j render partial: 'spree/admin/mail/forwards/row', locals: { mail_forward: @mail_forward}%>")
<%else%>
  $('#error_div').html("<%=j @response[:message].html_safe%>");
<%end%>