<% content_for :page_title do %>
 Mail Quota
<% end %>
<% content_for :page_actions do %>
<% end %>

<% if @mailbox_quotas.any? %>
<div data-nav-x-wrapper>
  <div data-nav-x-container>
    <table class="table" id="listing_orders" data-hook data-nav-x-content>
      <thead>
        <tr data-hook="admin_orders_index_headers">
          <th>Email Address</th>
          <th>Name</th>
          <th>Used Space</th>
          <th>Quota</th>
          <th>Used %</th>
        </tr>
      </thead>
      <tbody>
      <%@mailbox_quotas.each do |mailbox_quota| %>
        <tr data-hook="admin_orders_index_rows" class="<%= cycle('odd', 'even') %>">
          <td><%=mailbox_quota.email%></td>
          <td><%=mailbox_quota.name%></td>
          <td><%=mailbox_quota.used%></td>
          <td><%=mailbox_quota.quota%></td>
          <td><%=mailbox_quota.used_percentage%></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
<% else %>
  <div class="alert alert-info no-objects-found">
  
  </div>
<% end %>


<script type="text/javascript">
  
  $(function () {
  $('[data-toggle="tooltip"]').tooltip();
})
</script>

<script type="text/javascript">
  $(document).ready(function (argument) {
    $('#FlashAlertsContainer').removeClass('d-none');
  });
</script>
