<%if @response[:success] == true%>
  $("#myModal").modal('hide')
  $('#mailboxes_list_table #mailbox_id_<%=@mailbox.id%>').replaceWith("<%=j render partial: 'spree/admin/mail/mail_boxes/row', locals: { mailbox: @mailbox}%>")
<%else%>
  $('#error_div').html("<%=j @response[:message].html_safe%>");
<%end%>