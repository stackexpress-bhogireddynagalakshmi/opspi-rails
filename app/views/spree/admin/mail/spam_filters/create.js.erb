<%if @response[:success] == true%>
  $("#myModal").modal('hide')
  $('#spam_filter_<%=@spam_filter.wb%>_table').append("<%=j render partial: 'spree/admin/mail/spam_filters/row', locals: { spam_filter: @spam_filter}%>")
<%else%>
<%if @response[:message].include?('email_error_notempty')%>
$('#error_div').html("Error: Email should not be empty.");
<%else%>
  $('#error_div').html("<%=j @response[:message].html_safe%>");
<%end%>
<%end%>