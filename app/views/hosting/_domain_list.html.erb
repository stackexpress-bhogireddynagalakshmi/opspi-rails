<table class="table">
<%domains.each do |key, value|%>
  <%disabled = current_order.line_items.pluck(:domain).include?(key)%>
  <tr>
    <td><b><%=key%></b> is   <%=render_domain_status(value)%> <br/> </td>
      <%if value["status"] == 'available'%>
        <td><%=select_tag :years,options_for_select([['1 Year',1],['2 Year',2],['3 Year',3]])%></td>
        <td><%=number_to_currency(10)%></td>
        <td>
        <%= form_for :order, html: {
              id: 'add-to-cart-form',
              class: 'add-to-cart-form',
              'data-product-summary': @product_summary.to_json,
              'data-variants': [],
              'data-variant-change-trigger-identifier': 'productCarousel'
            } do |f| %>
         
            <%= hidden_field_tag "variant_id", @product.id %>
            <%=f.hidden_field :invoice_number,value: params[:invoice_number]%>
            <%=hidden_field_tag :domain,key%>
            <%= button_tag class: 'btn btn-primary w-100 text-uppercase font-weight-bold add-to-cart-button', id: 'add-to-cart-button', type: :submit,disabled: disabled do %>
                <%= Spree.t(:add_to_cart) %>
            <% end %>

          <% end %>
        <%else%>
          <td colspan="3"></td>
      <%end%>
      </td>
  </tr>
<%end%>
</table>
