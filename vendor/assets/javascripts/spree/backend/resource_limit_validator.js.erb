var checkResourceLimit = function(element){
  var error = document.getElementById("res_limit_error_message")
  var val = element.attributes.data.value
  if (val == 'true') {
      error.innerHTML = '<div class="alert alert-warning" role="alert">'+ '<%=I18n.t('spree.resource_limit_exceeds')%>' +'</div>';
      event.preventDefault();
      event.stopImmediatePropagation();
      return false;
  }
}